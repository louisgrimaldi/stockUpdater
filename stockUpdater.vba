Sub main()        ' Change the name of your data input worksheet here.    Worksheets("Sheet1").Activate            ' If needed, change the row of the first stock you need copied.    Range("A5").Select    Do Until IsEmpty(ActiveCell) = True        ActiveCell.Offset(0, 1).Select        r = r + 1    Loop    ActiveCell.Offset(0, -1).Select        Dim newRange As Range    ' If needed, change the Offset by the number of rows you want copied.    Set newRange = Range(ActiveCell, ActiveCell.Offset(95, 0))    newRange.Copy        Worksheets("Sheet2").Activate            ' If needed, change the row of the first stock you need pasted.    Range("A5").Select    Do Until IsEmpty(ActiveCell) = True        ActiveCell.Offset(0, 1).Select        r = r + 1    Loop        Worksheets("Sheet2").Activate    ActiveCell.Select    ActiveSheet.Paste        Application.CutCopyMode = False    End Sub